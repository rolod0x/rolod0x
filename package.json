{
  "name": "rolod0x",
  "version": "1.5.0",
  "description": "Your free private onchain address book",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rolod0x/rolod0x.git"
  },
  "bin": {
    "rolod0x": "./src/cli.ts"
  },
  "scripts": {
    "build": "tsc --noEmit && pnpm build:vite",
    "build:vite": "vite build",
    "build:firefox": "tsc --noEmit && pnpm build:firefox:vite",
    "build:firefox:vite": "cross-env __FIREFOX__=true vite build",
    "build:all": "tsc --noEmit && (run-p build:vite build:firefox:vite)",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "zip": "version=$(jq -r .version package.json); zip=rolod0x-v$version-chrome.zip; (cd dist/chrome && rm -f ../$zip && zip -r ../$zip *) && ls -l dist/$zip",
    "build:crx": "crx pack dist",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "devtools": "pnpm react-devtools",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "dev:all": "pnpm build:hmr && (run-p wss build:watch build:firefox:watch)",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:firefox": "pnpm web-ext run -s dist/firefox",
    "commitlint": "commitlint --edit",
    "lint": "run-p -c lint:eslint lint:prettier",
    "lint:watch": "while true; do fd -e ts -e tsx -e js | entr -d pnpm lint; done",
    "lint:fix": "run-p lint:eslint:fix lint:prettier:fix",
    "lint:eslint": "eslint src --max-warnings 0 --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "pnpm eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "tokenlist:import": "pnpm exec tsx ./src/import-tokenlist.ts",
    "prepare": "husky install",
    "rolod0x": "pnpm exec tsx ./src/cli.ts",
    "website:prepare": "./scripts/website-prepare.sh"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.5.0",
    "@commander-js/extra-typings": "^12.1.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/open-sans": "^5.1.0",
    "@fontsource/roboto": "^5.1.0",
    "@fontsource/ubuntu": "^5.1.0",
    "@fontsource/ubuntu-mono": "^5.1.0",
    "@lezer/highlight": "^1.2.1",
    "@mui/icons-material": "^5.16.11",
    "@mui/material": "^5.16.11",
    "@mui/system": "^5.16.8",
    "@types/lodash": "^4.17.13",
    "@types/webextension-polyfill": "^0.10.7",
    "@uiw/codemirror-themes": "^4.23.6",
    "@uiw/react-codemirror": "^4.23.6",
    "construct-style-sheets-polyfill": "^3.1.0",
    "dedent": "^1.5.3",
    "ethers": "^6.13.4",
    "lodash": "^4.17.21",
    "murmurhash": "^2.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.28.0",
    "tsx": "^4.19.2",
    "webext-dynamic-content-scripts": "^10.0.4",
    "webext-options-sync": "^4.3.0",
    "webext-permission-toggle": "5.1.0",
    "webext-permissions": "^3.1.4",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@codemirror/view": "^6.35.3",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.271",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "react-devtools": "^5.3.2",
    "rollup": "^4.28.1",
    "sass": "^1.83.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^2.1.8",
    "web-ext": "^8.3.0",
    "ws": "^8.18.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "packageManager": "pnpm@9.15.0",
  "peerDependencies": {
    "@codemirror/view": "^6.33.0"
  }
}
